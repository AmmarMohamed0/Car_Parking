    "import cv2\n",
    "import torch\n",
    "import numpy as np\n",
    "\n",
    "def detect_cars(frame, model, parking_area):\n",
    "    \"\"\"\n",
    "    Detect cars within the specified parking area in the given frame.\n",
    "    \n",
    "    Args:\n",
    "        frame (numpy.ndarray): Input frame.\n",
    "        model: YOLOv5 model for object detection.\n",
    "        parking_area (list): List of points defining the parking area polygon.\n",
    "        \n",
    "    Returns:\n",
    "        numpy.ndarray: Frame with cars detected within the parking area.\n",
    "        int: Number of cars detected within the parking area.\n",
    "    \"\"\"\n",
    "    # Perform object detection using the provided model\n",
    "    results = model(frame)\n",
    "    cars_in_parking = []\n",
    "    \n",
    "    # Iterate through each detected object\n",
    "    for index, row in results.pandas().xyxy[0].iterrows():\n",
    "        x1, y1, x2, y2 = int(row['xmin']), int(row['ymin']), int(row['xmax']), int(row['ymax'])\n",
    "        d = row['name'] # Detected object label\n",
    "        cx, cy = (x1 + x2) // 2, (y1 + y2) // 2  # Calculate centroid\n",
    "        \n",
    "        # Check if the detected object is a car\n",
    "        if 'car' in d:\n",
    "            # Check if the centroid of the car is within the parking area\n",
    "            if cv2.pointPolygonTest(np.array(parking_area, np.int32), (cx, cy), False) >= 0:\n",
    "                # Draw bounding box around the car\n",
    "                cv2.rectangle(frame, (x1, y1), (x2, y2), (0, 0, 255), 3)\n",
    "                # Add label to the car bounding box\n",
    "                cv2.putText(frame, str(d), (x1, y1), cv2.FONT_HERSHEY_PLAIN, 2, (255, 0, 0), 2)\n",
    "                cars_in_parking.append([cx])\n",
    "    \n",
    "    # Return the frame with detected cars and the number of cars detected\n",
    "    return frame, len(cars_in_parking)\n",
    "\n",
    "def main():\n",
    "    # Load YOLOv5 model\n",
    "    model = torch.hub.load('ultralytics/yolov5', 'yolov5m', pretrained=True)\n",
    "    \n",
    "    # Open video capture\n",
    "    cap = cv2.VideoCapture("parking.mp4")\n",
    "    \n",
    "    # Define parking area polygon\n",
    "    parking_area = [(32, 415), (9, 516), (389, 492), (777, 419), (713, 356)]\n",
    "    \n",
    "    while True:\n",
    "        ret, frame = cap.read()\n",
    "        if not ret:\n",
    "            break\n",
    "        \n",
    "        # Resize frame\n",
    "        frame = cv2.resize(frame, (1020, 600))\n",
    "        \n",
    "        # Detect cars in frame\n",
    "        frame, num_cars = detect_cars(frame, model, parking_area)\n",
    "        \n",
    "        # Draw parking area and display number of cars\n",
    "        cv2.polylines(frame, [np.array(parking_area, np.int32)], True, (0, 255, 0), 2)\n",
    "        cv2.putText(frame, 'Number of cars in parking = ' + str(num_cars), (50, 80), cv2.FONT_HERSHEY_PLAIN, 2, (0, 0, 255), 2)\n",
    "        \n",
    "        # Display frame\n",
    "        cv2.imshow(\"FRAME\", frame)\n",
    "        \n",
    "        # Break loop if 'Esc' key is pressed\n",
    "        if cv2.waitKey(30) == 27: # 30-> frame rete \n",
    "            break\n",
    "\n",
    "    # Release video capture and close windows\n",
    "    cap.release()\n",
    "    cv2.destroyAllWindows()\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
